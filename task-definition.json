{
  "family": "shr-nextjs-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::607336804654:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "nextjs-container",
      "image": "IMAGE_PLACEHOLDER",
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ]
}
